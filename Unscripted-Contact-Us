    <script type="text/javascript" src="https://maps.googleapis.com/maps/api/js?key=AIzaSyBn_8puekrsQBA2Esco2sHrLEFuK2fRvbw&amp;libraries=places"></script>
    <script type="text/javascript">
        $(function () {
            // Handle date type
            $('input[name=dateType]').on('change', function() {
                if ($(this).val() === 'flexible') {
                    specificDate.hide();
                }
            });


            // Calendar date
            var dateSelector = '#book-datepicker';
            var startTimeSelector = '#book-start-timepicker';
            var endTimeSelector = '#book-end-timepicker';

            $(dateSelector).datetimepicker({
                inline: true,
                sideBySide: true,
                format: 'L',
                viewDate: 'string'
            });
            $(startTimeSelector).datetimepicker({
                format: 'LT'
            });
            $(endTimeSelector).datetimepicker({
                format: 'LT'
                // format: 'HH:mm',
                // useCurrent: false,
            });
            var specificDate = $('#scheduled-date');
            $('#calendar-modal-save').on('click', function() {
                    if ($(startTimeSelector + ' input').val() === '' && $(endTimeSelector + ' input').val() === '') {
                        specificDate.val( $(dateSelector + ' input').val() );
                    } else {
                        specificDate.val( $(dateSelector + ' input').val() + ', ' + $(startTimeSelector + ' input').val() + ' - ' + $(endTimeSelector + ' input').val() );
                    }
                    specificDate.show();


                if ( !($(startTimeSelector + ' input').val().length > 1 && $(endTimeSelector + ' input').val().length === 0) ) {
                    $('#calendar-modal').modal('hide');
                }
            });

            // Check flexible radio when specific is cancelled
            $(document).on('click', '[data-dismiss="modal"]', function(){
                $('input[value=flexible]').prop("checked", true);
            });

            // Enable/Disable end time
            $(endTimeSelector + ' input').prop('disabled', true);
            $(startTimeSelector + ' input').on("input", function (e) {
                if ($(this).val().length > 0) {
                    $(endTimeSelector + ' input').prop('disabled', false);
                } else {
                    $(endTimeSelector + ' input').prop('disabled', true);
                }
            });


            // Location places
            var input = document.getElementById("location");
            var autocomplete = new google.maps.places.Autocomplete(input, {});
            autocomplete.addListener("place_changed", function () {
                var place = autocomplete.getPlace();
                $("#location-name").val(place.formatted_address);
                $("#location-lat").val(place.geometry.location.lat());
                $("#location-lng").val(place.geometry.location.lng());
            });
        });
    </script>


    <script type="text/javascript" src="https://unscriptedforphotographers.com/vendor/jsvalidation/js/jsvalidation.js"></script>
    <script>
    jQuery(document).ready(function(){

        $("#lead-form").each(function() {
            $(this).validate({
                errorElement: 'span',
                errorClass: 'help-block error-help-block',

                errorPlacement: function (error, element) {
                    if (element.parent('.input-group').length ||
                        element.prop('type') === 'checkbox' || element.prop('type') === 'radio') {
                        error.insertAfter(element.parent());
                        // else just place the validation message immediately after the input
                    } else {
                        error.insertAfter(element);
                    }
                },
                highlight: function (element) {
                    $(element).closest('.form-group').removeClass('has-success').addClass('has-error'); // add the Bootstrap error class to the control group
                },

                
                ignore: ":hidden, [contenteditable='true']",
                
                /*
                 // Uncomment this to mark as validated non required fields
                 unhighlight: function(element) {
                 $(element).closest('.form-group').removeClass('has-error').addClass('has-success');
                 },
                 */
                success: function (element) {
                    $(element).closest('.form-group').removeClass('has-error').addClass('has-success'); // remove the Boostrap error class from the control group
                },

